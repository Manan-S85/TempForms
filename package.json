{
  "name": "tempforms",
  "version": "1.0.0",
  "description": "Temporary forms with auto-expiring functionality - privacy-focused and anonymous",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-json": "concurrently \"npm run server-json\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "server-json": "cd backend && npm run dev-json",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "forms",
    "temporary",
    "anonymous",
    "privacy",
    "auto-delete",
    "survey",
    "feedback"
  ],
  "author": "TempForms",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "mongodb": "^7.0.0"
  }
}
